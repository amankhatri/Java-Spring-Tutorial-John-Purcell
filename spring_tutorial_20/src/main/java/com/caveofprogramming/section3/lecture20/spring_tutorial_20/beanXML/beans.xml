<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean autowire="constructor" id="logger"
		class="com.caveofprogramming.section3.lecture20.spring_tutorial_20.Logger"> </bean>

	<bean name="consoleWriter"
		class="com.caveofprogramming.section3.lecture20.spring_tutorial_20.ConsoleWriter"> </bean>

	<bean id="fileWriter"
		class="com.caveofprogramming.section3.lecture20.spring_tutorial_20.FileWriter"> </bean>





</beans>
